  "scripts": {
    "start": "yarn counter && cp ./src/meta.json public && react-app-rewired start",
    "build": "yarn counter && react-app-rewired build && yarn cp:meta",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "lint:fix": "eslint ./src --ext .js,.jsx --fix",
    "lint": "eslint ./src --ext .js,.jsx",
    "analyze": "react-app-rewired build --prod --analyze",
    "counter": "node ./scripts/buildNumber.js",
    "cp:meta": "cp ./src/meta.json build",
    "build:prod": "yarn counter && react-app-rewired build --prod && yarn cp:meta",
    "sync:prod": "env-cmd -f .env.production node ./scripts/deploy.js",
    "deploy:prod": "yarn build:prod && yarn sync:prod",
    "build:staging": "yarn counter && env-cmd -f .env.staging react-app-rewired build && yarn cp:meta",
    "sync:staging": "env-cmd -f .env.staging node ./scripts/deploy.js",
    "deploy:staging": "yarn build:staging && yarn sync:staging"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
